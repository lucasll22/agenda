<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lucas2.agenda">

    <application
        android:name=".AppLifeCycle"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainScreen"
            android:label="Agenda" />
        <activity
            android:name=".Materias"
            android:label="Meterias" />
        <activity
            android:name=".Provas"
            android:label="Provas" />
        <activity
            android:name=".Trabalhos"
            android:label="Trabalhos" />
        <activity android:name=".NovaMateria" />
        <activity android:name=".NovaProva" />
        <activity android:name=".NovoTrabalho"></activity>
    </application>

</manifest>